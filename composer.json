{
    "name": "botman/studio-addons",
    "license": "MIT",
    "description": "BotMan Studio specific addons.",
    "keywords": [
        "Bot",
        "BotMan",
        "Studio"
    ],
    "homepage": "http://github.com/botman/studio-addons",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "m.pociot@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "botman/botman": "dev-laravel7",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/contracts": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0",
        "thecodingmachine/discovery": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "^3.6|^3.7|^3.8|^4.0|^5.0",
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "BotMan\\Studio\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BotMan\\Studio\\Providers\\StudioServiceProvider",
                "BotMan\\Studio\\Providers\\RouteServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "cs": "php-cs-fixer fix"
    },
    "repositories": [
      {
          "type": "vcs",
          "url": "https://github.com/mikemiller891/botman-botman.git"
      }
  ]
}
